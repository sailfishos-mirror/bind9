@deletion1@
statement S1, S2;
expression V;
@@
-V =
     isc_buffer_allocate(...);
(
-if (V != ISC_R_SUCCESS) S1
|
-if (V == ISC_R_SUCCESS)
    S1
-else S2
|
-check_result(V, ...);
|
-assert_int_equal(V, ISC_R_SUCCESS);
|
-CHECK(..., V);
|
-RUNTIME_CHECK(V == ISC_R_SUCCESS);
)

